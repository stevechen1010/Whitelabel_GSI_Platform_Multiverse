{
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.formatOnSave": true,
  "files.trimTrailingWhitespace": true,
  "prettier.semi": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never"
  },
  "eslint.format.enable": true,
  "eslint.validate": ["javascript", "javascriptreact", "html", "vue", "typescript", "typescriptreact"],
  "eslint.enable": false,
  "typescript.tsdk": "node_modules/typescript/lib",
  "[sass]": {
    "editor.defaultFormatter": "michelemelluso.code-beautifier",
    "editor.formatOnSave": false
  },
  "i18n-ally.localesPaths": ["src/i18n/locales"],
  "i18n-ally.keystyle": "nested",
  "i18n-ally.sortKeys": true,
  "i18n-ally.namespace": true,
  "i18n-ally.readonly": false,
  "i18n-ally.sourceLanguage": "en-US",
  "i18n-ally.displayLanguage": "zh-CN",
  "i18n-ally.enabledParsers": ["ts"],
  "i18n-ally.enabledFrameworks": ["vue"],
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.foldingStrategy": "indentation"
  },
  "[scss]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "i18n-ally.extract.ignored": ["Product Category"],
  "i18n-ally.extract.ignoredByFiles": {
    "src\\i18n\\locales\\en-US.ts": ["Product Category"]
  },
  "vue-i18n.i18nPaths": "src/i18n,src/i18n/locales"
}
